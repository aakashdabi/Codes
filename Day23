public class DailyCode {
    public static int fib(int num){
    if(num==0)
    return 0;
        if(num==2|| num==1 ){
            return 1;
        }
         return fib(num-2)+fib(num-1);
    }
    public static void main(String[] args) {
      int num=5;
      System.out.println(fib(num));
    }
}
