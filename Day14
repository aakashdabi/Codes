public class DailyCode{
    public static void main(String[] args) {
        int arr[]={22,5,3,8,5,4,5,43,5};
        int target=5;
        int count=0;

        for(int i=0; i<arr.length; i++){
            if(arr[i]==target){
                count++;
            }
        }
       System.out.println("The number of occurrences of the target element in the arrray is "+count);
    }
}
