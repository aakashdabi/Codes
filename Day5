public class Daily {
  public static void main(String[] args) {
    int arr[]=new int[10];
    arr[0]=0; //or -> arr[0]=1;
    arr[1]=1;
    for(int i=0; i<=10; i++){
        if(i==0|| i==1){
            System.out.print(arr[i]+" ");
        }
        else{
            arr[i]=arr[i-1]+arr[i-2];
            System.out.print(arr[i]+" ");
        }
    }
    System.out.println();
  }
}
